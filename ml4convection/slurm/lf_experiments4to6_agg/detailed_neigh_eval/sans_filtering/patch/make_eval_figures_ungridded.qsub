#!/bin/tcsh

#SBATCH --job-name="make_eval_figures_ungridded"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="batch"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
##SBATCH --nodes=1
##SBATCH --ntasks=1
##SBATCH --cpus-per-task=1
##SBATCH --ntasks-per-node=1
#SBATCH --time=01:00:00
#SBATCH --array=1-16
#SBATCH --exclude=h28n12
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=make_eval_figures_ungridded_%A_%a.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4convection_standalone/ml4convection"
set ALL_EXPERIMENT_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4convection_models"
set TOP_OUTPUT_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4convection_project/loss_function_paper/experiments4to6_agg/detailed_neigh_eval/sans_filtering"

set SCORE_NAMES=("fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss" "fss")
set FILTER_NAMES=("_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000" "_wavelets0_min-resolution-deg=0.0000_max-resolution-deg=0.0500" "_wavelets0_min-resolution-deg=0.0500_max-resolution-deg=10000000000.0000")
set MATCHING_DISTANCES_PX=(0 0 1 1 2 2 3 3 4 4 6 6 8 8 12 12)
set SUBEXPERIMENT_ENUMS=("05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05" "05")

set score_name=${SCORE_NAMES[$SLURM_ARRAY_TASK_ID]}
set filter_name=${FILTER_NAMES[$SLURM_ARRAY_TASK_ID]}
set subexperiment_enum=${SUBEXPERIMENT_ENUMS[$SLURM_ARRAY_TASK_ID]}
set matching_distance_px=${MATCHING_DISTANCES_PX[$SLURM_ARRAY_TASK_ID]}
set matching_distance_string=`printf "%.6f" ${matching_distance_px}`

set prediction_dir_names={${ALL_EXPERIMENT_DIR_NAME}/lf_experiment${subexperiment_enum}/${score_name}${filter_name}/model*/validation_best_validation_loss/full_grids}
set prediction_dir_name=${prediction_dir_names[$#prediction_dir_names]}

set temporal_eval_dir_name="${prediction_dir_name}/evaluation_sans_filtering/matching_distance_px=${matching_distance_string}"
set non_temporal_eval_dir_name="${temporal_eval_dir_name}/advanced_scores_gridded=0"
set output_dir_name="${TOP_OUTPUT_DIR_NAME}/${score_name}${filter_name}/matching_distance_px=${matching_distance_string}"

python3 -u "${CODE_DIR_NAME}/make_eval_figure_ungridded.py" \
--input_temporal_eval_dir_name="${temporal_eval_dir_name}" \
--input_non_temporal_eval_dir_name="${non_temporal_eval_dir_name}" \
--output_dir_name="${output_dir_name}"
